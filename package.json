    "lodash.merge": "^4.6.2",
    "nunjucks": "^3.2.4",
    "typescript": "~5.8.3",
    "vite": "^7.0.3",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^22.13.14",
    "@types/nunjucks": "^3.2.6",
    "@vitejs/plugin-vue": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "sass-embedded": "^1.89.2",
    "typescript": "^5.8.3"
  }
}
