{
  "name": "vite-plugin-styleguide",
  "version": "0.2.0",
  "description": "",
  "main": "dist/plugin/vite-plugin-styleguide.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.plugin.json && vite build",
    "watch:app": "vite build --watch",
    "watch:plugin": "tsc -p tsconfig.plugin.json --watch",
    "watch": "npm-run-all --parallel watch:app watch:plugin",
